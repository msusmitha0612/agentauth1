{
  "name": "agentauth",
  "version": "0.1.0",
  "description": "OAuth token management for AI agents",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup index.ts --format cjs,esm --dts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "oauth",
    "ai",
    "agents",
    "authentication",
    "tokens",
    "google",
    "api"
  ],
  "author": "AgentAuth",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/agentauth/agentauth-js"
  },
  "homepage": "https://agentauth.dev",
  "bugs": {
    "url": "https://github.com/agentauth/agentauth-js/issues"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  }
}
